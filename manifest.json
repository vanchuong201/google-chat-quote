{
"manifest_version": 3,
    "name": "Google Chat Quote",
    "description": "Add Google Chat Quote",
    "version": "0.0.2",
    "content_scripts": [
        {
            "matches": [
                "https://chat.google.com/*",
                "https://mail.google.com/chat/*"
            ],
            "all_frames": true,
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
